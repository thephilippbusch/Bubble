# Server Env Variables
SERVER_JWT_SECRET=jwt_secret # Secret for Token generation
SERVER_JWT_ALGORITHM=jwt_algorithm # i.e. HS256, HS384, HS512. More Info: https://pypi.org/project/jwt/
SERVER_ENV=development # development or production
SERVER_ADMIN_UID=some_uid # UID for initial admin user generated by backend server
SERVER_ADMIN_PASSWORD=some_password # Password for initial admin user generated by backend server

# Server Env Variables for DB Connection

# Following URL Connection String is special for PostgreSQL
# (postgres_database = Docker Container Name)
DATABASE_URL=postgresql+psycopg2://<DATABASE_USER>:<DATABASE_PASSWORD>@postgres_database:<DATABASE_PORT>/<DATABASE_NAME>
# Using the example values below:
# DATABASE_URL=postgresql+psycopg2://some_username:some_password@postgres_database:port/database_name

DATABASE_HOST=some_host
DATABASE_NAME=database_name
DATABASE_USER=some_username
DATABASE_PASSWORD=some_password
DATABASE_PORT=port